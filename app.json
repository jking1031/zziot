{
  "expo": {
    "name": "正泽物联",
    "slug": "zziot20",
    "version": "1.5.1",
    "orientation": "portrait",
    "icon": "./assets/logo3.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zziot.app",
      "buildNumber": "1.5.1",
      "requireFullScreen": true,
      "infoPlist": {
        "NSCameraUsageDescription": "该应用需要访问您的相机用于拍摄照片和扫描二维码",
        "NSPhotoLibraryUsageDescription": "该应用需要访问您的照片库用于上传和保存图片",
        "NSPhotoLibraryAddUsageDescription": "该应用需要保存图片到您的照片库以便后续查看",
        "NSLocationWhenInUseUsageDescription": "该应用需要访问您的位置来提供基于位置的服务和天气信息",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "该应用需要在后台访问您的位置来提供实时位置服务和天气更新",
        "NSLocationAlwaysUsageDescription": "该应用需要在后台访问您的位置来提供持续的位置服务",
        "NSMicrophoneUsageDescription": "该应用需要访问您的麦克风用于语音功能",
        "NSContactsUsageDescription": "该应用需要访问您的通讯录以便分享和联系",
        "NSCalendarsUsageDescription": "该应用需要访问您的日历以便添加和管理日程",
        "NSRemindersUsageDescription": "该应用需要访问您的提醒事项以便设置提醒",
        "NSMotionUsageDescription": "该应用需要访问您的运动与健身数据以提供相关功能",
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiresFullScreen": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": true,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true
        }
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "a96d138c-2d47-49b1-845e-fd69f0d70c2a"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "owner": "mrking1031",
    "android": {
      "package": "com.zziot.app",
      "versionCode": 146,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo3.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECORD_AUDIO",
        "READ_CONTACTS",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "ACTIVITY_RECOGNITION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "allowBackup": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0",
            "enableProguardInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ]
    ]
